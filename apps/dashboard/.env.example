# Plinto Dashboard Environment Variables
# Copy this file to .env.local and fill in the values

# API Configuration
NEXT_PUBLIC_API_URL=https://api.plinto.dev
NEXT_PUBLIC_API_VERSION=v1

# Authentication
NEXT_PUBLIC_PLINTO_ISSUER=https://plinto.dev
NEXT_PUBLIC_PLINTO_AUDIENCE=plinto.dev
NEXT_PUBLIC_PLINTO_TENANT_ID=tenant_123
NEXT_PUBLIC_PLINTO_JWKS_URL=https://plinto.dev/.well-known/jwks.json

# App Configuration
NEXT_PUBLIC_APP_URL=https://app.plinto.dev
NEXT_PUBLIC_APP_NAME="Plinto Dashboard"

# Feature Flags
NEXT_PUBLIC_ENABLE_WEBHOOKS=false
NEXT_PUBLIC_ENABLE_AUDIT_LOG=false
NEXT_PUBLIC_ENABLE_ANALYTICS=true

# Session Configuration
SESSION_SECRET=your-session-secret-here
SESSION_MAX_AGE=86400

# Monitoring (optional)
NEXT_PUBLIC_SENTRY_DSN=
NEXT_PUBLIC_POSTHOG_KEY=